@model HR.Models.IR8AViewModel
@using HR.Models
@using HR.Common
@using SBSModel.Models;
@using SBSModel.Common;
@using SBSResourceAPI;
@{
    ViewBag.Title = Resource.IR8A;
    var currentdate = StoredProcedure.GetCurrentDate();

    string ref_no = Model.Employee_Profile.NRIC;
    if (ref_no.IsEmpty())
    {
        ref_no = "FIN/ " + Model.Employee_Profile.FIN_No;
    }
    else
    {
        ref_no = "NRIC/ " + ref_no;
    }

    String nationality = "";
    if (Model.Employee_Profile != null && Model.Employee_Profile.Nationality != null)
    {
        nationality = Model.nationalityList.Where(w => w.Value == Model.Employee_Profile.Nationality_ID.ToString()).FirstOrDefault().Text;
    }

    var year = Model.sYear - 1;
}



<script type="text/javascript">
    $(function () {
        InitDatepicker();
    });
</script>
<script type="text/javascript">
    $(function () {

        @if (Model.operation == UserSession.RIGHT_U) {
            @:Disable('ir8a');
                                                                                                                                                                                                                                                                                                                                           }

        $('input[name=Income_Tax_Employer]:radio').click(function () {
            setIncome_Tax_Employer();
        });

        $('input[name=Approval_IRAS]:radio').click(function () {
            if (this.getAttribute("readonly") == null || this.getAttribute("readonly") == '')
                setDate_Approval();
        });

        setIncome_Tax_Employer();
        setDate_Approval();


        $('#Allowance_Transport').change(function () {
            sumAullowance();
        });

        $('#Allowance_Entertain').change(function () {
            sumAullowance();
        });

        $('#Allowance_Others').change(function () {
            sumAullowance();
        });

        sumAullowance();

        $('#Gratuity').change(function () {
            lumpSum();
        });

        $('#Notice_Pay').change(function () {
            lumpSum();
        });

        $('#Ex_Gratia').change(function () {
            lumpSum();
        });

        $('#Lump_Sum_Others').change(function () {
            lumpSum();
        });

        $('#Compensation_Loss').change(function () {
            lumpSum();
        });

        lumpSum();

        $('#Allowance').change(function () {
            sumD();
        });
        $('#Commission_Amount').change(function () {
            sumD();
        });
        $('#Pension').change(function () {
            sumD();
        });
        $('#Lump_Sum').change(function () {
            sumD();
        });
        $('#Amount_Accured_1992').change(function () {
            sumD();
        });
        $('#Amount_Accured_1993').change(function () {
            sumD();
        });
        $('#Contribution_Out_Singapore').change(function () {
            sumD();
        });
        $('#Excess_Less').change(function () {
            sumD();
        });
        $('#Excess_Employer').change(function () {
            sumD();
        });
        $('#Gain_Profit').change(function () {
            sumD();
        });
        $('#Value_Benefit').change(function () {
            sumD();
        });

        sumD();

        Commission_Onchage();
    });


    function setIncome_Tax_Employer() {
        if ($('input[name=Income_Tax_Employer][value=false]').prop('readonly') == false) {
            if ($('input[name=Income_Tax_Employer][value=false]').prop('checked') == true) {
                $('#Tax_Partially').prop('readonly', true);
                $('#Tax_Fixed').prop('readonly', true);
            }
            else {
                $('#Tax_Partially').prop('readonly', false);
                $('#Tax_Fixed').prop('readonly', false);
                $('#Tax_Partially').val("0.00");
                $('#Tax_Fixed').val("0.00");
            }
        }

    }

    function setDate_Approval() {
        if ($('input[name=Approval_IRAS][value=true]').prop('checked') == true) {
            $('#Date_Approval').prop('readonly', false);
        }
        else {
            $('#Date_Approval').prop('readonly', true);
            $('#Date_Approval').val("");
        }
    }

    function sumAullowance() {

        total = parseFloat($('#Allowance_Transport').val()) +
            parseFloat($('#Allowance_Entertain').val()) +
            parseFloat($('#Allowance_Others').val());
        $('#Allowance').val(parseFloat(total).toFixed(2));
    }

    function lumpSum() {
        total = parseFloat($('#Gratuity').val()) +
            parseFloat($('#Notice_Pay').val()) +
            parseFloat($('#Ex_Gratia').val()) +
            parseFloat($('#Lump_Sum_Others').val()) +
            parseFloat($('#Compensation_Loss').val());
        $('#Lump_Sum').val(parseFloat(total).toFixed(2));

    }

    function sumD() {
        total = parseFloat($('#Allowance').val()) +
            parseFloat($('#Commission_Amount').val()) +
            parseFloat($('#Pension').val()) +
            parseFloat($('#Lump_Sum').val()) +
            parseFloat($('#Amount_Accured_1992').val()) +
            parseFloat($('#Amount_Accured_1993').val()) +
            parseFloat($('#Contribution_Out_Singapore').val()) +
            parseFloat($('#Excess_Less').val()) +
            parseFloat($('#Excess_Employer').val()) +
            parseFloat($('#Gain_Profit').val()) +
            parseFloat($('#Value_Benefit').val());

        $('#sumD').val(parseFloat(total).toFixed(2));
    }

    function Commission_Onchage() {
        $("#Commission_Amount").val(0);
        var sdate = $("#Commission_Start").val();
        var edate = $("#Commission_End").val();
        var eid = '@Model.Employee_Profile_ID';
        $.ajax({
            type: 'POST',
            url: 'IR8ACommissionAmount',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify({
                sdate: sdate,
                edate: edate,
                eid: eid
            }),
            dataType: 'json',
            success: function (data) {
                if (data != null) {
                    $("#Commission_Amount").val(data.amount);
                }
                @if(Model.operation == UserSession.RIGHT_U) {
                    @:$("#Commission_Amount").val(parseFloat($("#Commission_Amount").val()).toFixed(2));
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     }

            }
        });
    }
</script>

