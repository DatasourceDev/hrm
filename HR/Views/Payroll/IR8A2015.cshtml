                                <a href="#" class="minimize"><i class="fa fa-chevron-down color-white"></i></a>
                            </div>
                        </div>
                        <div class="tile-body">
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <label for="Company_Name" class="control-label">@Resource.Name_Of_Employer</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <label for="Company_Address" class="control-label"> @Resource.Address_Of_Employer</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <label for="User_Name" class="control-label">@Resource.Message_Name_Of_Authorised_Person_Making_Declaration</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <input type="text" class="form-control " id="Company_Name" value="@Model.company.Name" readonly>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <label for="Designation" class="control-label">@Resource.Designation</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <input type="text" class="form-control " id="Designation" value="@Model.employer_Desination_Name" readonly>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <input type="text" class="form-control" id="Company_Address" value="@Model.company.Address" readonly>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <label for="Tel" class="control-label">@Resource.Tel</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <input type="text" class="form-control" id="Tel" value="@Model.employer.Mobile_No" readonly>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <div class="col-md-12">
                                            <input type="text" class="form-control " id="User_Name" value="@AppConst.GetUserName(Model.employer.User_Profile)" readonly>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>

            <div class="row">
                <div class="col-md-12">
                    <section class="tile color transparent-white">
                        <div class="tile-body">
                            <div class="row">
                                <div class="col-md-12">
                                    @if (Model.operation == UserSession.RIGHT_C)
                                    {
                                        <button type="submit" class="btn btn-primary">@Resource.Save</button>
                                    }
                                    @if ((Model.operation == UserSession.RIGHT_U || Model.operation == UserSession.RIGHT_A) && Model.iid != 0)
                                    {
                                        <a id="preview" class="btn btn-primary link-allow-click" target="_blank" href="@Url.Action("IR8APdf", new { operation = EncryptUtil.Encrypt(UserSession.RIGHT_A),  iid = EncryptUtil.Encrypt(Model.iid) })"> @Resource.Preview_IR8A</a>
                                    }

                                    <a href="@Url.Action("IR8A", "Payroll", new { })" class="btn btn-default link-allow-click">@Resource.Back</a>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </div>
    </div>
}


