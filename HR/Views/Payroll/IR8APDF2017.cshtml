            <td>
                <p align=center style='text-align:center;'>
                    <span style='color:white'>
                        @Resource.Message_Do_Not_Submit
                    </span>
                </p>
            </td>
        </tr>
    </table>

    <p>
        <span style='color: black; padding-left: 5px; padding-right: 5px; padding-top: 5px; padding-bottom: 5px; '>
            @Resource.Message_This_Form_Will_Take_About_10_Minutes
        </span>
    </p>

    <p align=center style='text-align:center;line-height:5.0pt;'>
        <span style='font-size:6.0pt;'>&nbsp;</span>
    </p>
    <table border="1" cellspacing=1 cellpadding=1 style='width:100%; border-collapse:collapse;'>
        <tr>
            <td width="45%" valign=top style="padding-left: 3px;">
                <p>
                    <span>
                        @Resource.Employees_Tax_Ref_No/ @Resource.UEN
                        <span></span>
                    </span>
                </p>
                <p>
                    @Model.company.Registry
                </p>
            </td>
            <td width="55%" style="padding-left: 3px;" valign=top>
                <p>
                    <span>
                        @Resource.Employees_Tax_Ref_No :
                    </span> <!-- *NRIC / FIN (Foreign Identification No.) -->
                    @{
                        string ref_no = Model.Employee_Profile.NRIC;
                        if (ref_no.IsEmpty())
                        {
                            ref_no = "FIN (Foreign Identification No.) <br> " + Model.Employee_Profile.FIN_No;
                        }
                        else
                        {
                            ref_no = "NRIC  <br>" + ref_no;
                        }

                    }
