@model HR.Models.IR8AViewModel
@using HR.Models
@using SBSModel.Models;
@using SBSModel.Common;
@using SBSResourceAPI;

@{
    ViewBag.Title = @Resource.Payroll_PDF;
    Layout = null;

    String nationality = "";
    if (Model.Employee_Profile != null && Model.Employee_Profile.Nationality != null)
        nationality = Model.nationalityList.Where(w => w.Value == Model.Employee_Profile.Nationality_ID.ToString()).FirstOrDefault().Text;

}


<div style="line-height: 9.3pt; font-size: 8pt; padding-left: 0pt;text-align: left; font-family: 'Arial','sans-serif';">
    <table>
        <tr>
            <td width="25%">
                <b>
                    <span style='font-size:20.0pt;'>
                        @Model.P_YEAR
                    </span>
                </b>
            </td>
            <td width="50%">
                <p align="center" style="padding-left: 0pt; ">

                    <b></b>
                    <b>
                        <span style='font-size:16.0pt;'>
                            @Resource.FORM_IR8A
                        </span>
                    </b>
                </p>
            <td width="25%"></td>
        </tr>
    </table>


    <table border=0 cellspacing=1 cellpadding=0 style='width:100.7%;border-collapse:collapse;border:none'>
        <tr bgcolor="#000">
            <td>
                <p align=center style='text-align: center;'>
                    <span style='color:white'>
                        @Resource.Message_Return_Of_Employee_Remuneration_For_The_Year_Ended_31_Dec @( Int32.Parse(Model.P_YEAR) - 1)
                    </span>
                </p>
            </td>
        </tr>
        <tr bgcolor="#000">
            <td>
                <p align=center style='text-align:center;'>
